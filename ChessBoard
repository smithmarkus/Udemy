public class Main {
    public static void main(String[] args) {
        int i = 0;
        int j = 0;
        for (i = 0; i < 8; i++) {
            System.out.println();
            for (j = 0; j < 8; j++) {
                if ((i + j) % 2 == 0) {
                    System.out.print("◼️");
                } else {
                    System.out.print("◻️");
                }
            }
        }
    }
}
